    "workbench.colorTheme": "Mud Light (rainglow)",
    "gitlens.hovers.currentLine.over": "line",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "gitlab.enableExperimentalFeatures": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "git.autofetch": true,
    "todo-tree.tree.showScanModeButton": false,
    "editor.minimap.enabled": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "window.newWindowDimensions": "inherit",
    "projectManager.openInNewWindowWhenClickingInStatusBar": true,
    "git.pruneOnFetch": true,
    "editor.inlineSuggest.enabled": true,
    "diffEditor.renderSideBySide": false,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "explorer.experimental.fileNesting.patterns": {
        "*.ts": "$(capture).js, $(capture).*.ts",
        "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
        "*.jsx": "$(capture).js",
        "*.tsx": "$(capture).ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, .npmrc, yarn.lock, .yarnrc, pnpm-lock.yaml, .eslint*, .prettier*"
    },
    "explorer.experimental.fileNesting.enabled": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "rust-analyzer.lens.implementations": false,
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": false,
    "i18n-ally.namespace": true,
    "i18n-ally.pathMatcher": "static/modules/{namespace}/locales/{locale}.{ext}",
    "i18n-ally.enabledParsers": [
        "js"
    ],
}